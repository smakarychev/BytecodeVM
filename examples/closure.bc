let println_count;

fun counter()
{
  let count = 0;
  fun fib(n)
  {
    count = count + 1;
    if (n <= 1) return 1;
    return fib(n - 1) + fib(n - 2);
  }
  fun p()
  {
    println('{}', count);
  }
  println_count = p;
  return fib;
}

let fib = counter();

let beg = clock();

fib(36);

let end = clock();
println_count();
let duration = (end - beg) / 1000.0;
println('Duration in sec: {}', duration);