fun mandelbrot(x_min, x_max, y_min, y_max, width, height, max_iter)
{
    for (var i_y = 0; i_y < height; i_y = i_y + 1)
    {
        var row = "";
        for (var i_x = 0; i_x < width; i_x = i_x + 1)
        {
            var x_percent = i_x / width;
            var y_percent = i_y / height;
            var cr = x_min + (x_max - x_min) * x_percent;
            var ci = y_min + (y_max - y_min) * y_percent;
            var iter = mandelbrot_at_point(cr, ci, max_iter);
            row = row + mandelbrot_shade(iter);
        }
        print row;
    }
}


