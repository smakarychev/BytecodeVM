
fun complex_norm(cr, ci) { return cr * cr + ci * ci; }

fun mandelbrot_at_point(cr, ci, max_iter)
{
    var zr = 0;
    var zi = 0;
    var znorm = complex_norm(zr, zi);
    var iter = 0;
    while (znorm < 200.0 and iter < max_iter)
    {
        var zsr = zr * zr - zi * zi;
        var zsi = 2 * zr * zi;
        zr = zsr + cr;
        zi = zsi + ci;
	    znorm = complex_norm(zr, zi);
        iter = iter + 1;
    }
    return iter;
}

